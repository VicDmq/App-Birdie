{
  "name": "app-birdie",
  "version": "1.0.0",
  "description": "Birdie application - Victor Domecq",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "set NODE_ENV=production&& babel-node src/server/index.js",
    "heroku-prebuild": "set NPM_CONFIG_PRODUCTION=false",
    "heroku-postbuild": "set NPM_CONFIG_PRODUCTION=true && npm run build",
    "client": "webpack-dev-server --open --mode development --hot",
    "server": "nodemon --exec babel-node src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "mocha --require @babel/polyfill --require @babel/register"
  },
  "engines": {
    "node": "11.5.0",
    "npm": "6.4.1"
  },
  "dependencies": {  
    "@babel/core": "^7.1.0",
    "@babel/node": "^7.2.2",
    "express": "^4.16.4",
    "mysql": "^2.16.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-loader-spinner": "^2.3.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.8",
    "concurrently": "^4.1.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9"
  },
  "author": "Victor Domecq",
  "license": "ISC"
}
